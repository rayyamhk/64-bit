  },
  "homepage": "https://github.com/rayyamhk/base64-bit#readme",
  "bugs": {
    "url": "https://github.com/rayyamhk/base64-bit/issues"
  },
  "author": "Ray Yam",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "jest": "^28.1.0",
    "uglify-js": "^3.15.5"
  }
}
